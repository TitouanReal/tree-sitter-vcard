{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "vcard",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "property"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "STRING",
                    "value": "\r\n"
                  },
                  {
                    "type": "STRING",
                    "value": "\n"
                  }
                ]
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "property"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "property": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "group"
                },
                {
                  "type": "STRING",
                  "value": "."
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "property_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ";"
              },
              {
                "type": "SYMBOL",
                "name": "parameter"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "property_value"
        }
      ]
    },
    "group": {
      "type": "PATTERN",
      "value": "[A-Za-z0-9-]+"
    },
    "property_name": {
      "type": "PATTERN",
      "value": "[A-Za-z0-9-]+"
    },
    "property_value": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "[^\\r\\n]"
          },
          {
            "type": "STRING",
            "value": "\r\n "
          },
          {
            "type": "STRING",
            "value": "\r\n\t"
          },
          {
            "type": "STRING",
            "value": "\n "
          },
          {
            "type": "STRING",
            "value": "\n\t"
          }
        ]
      }
    },
    "parameter": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "parameter_name"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "parameter_value"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "parameter_value"
              }
            ]
          }
        }
      ]
    },
    "parameter_name": {
      "type": "PATTERN",
      "value": "[A-Za-z0-9-]+"
    },
    "parameter_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "[^\";:,\\x00-\\x1F]+"
        },
        {
          "type": "PATTERN",
          "value": "\"[^\"\\x00-\\x1F]+\""
        }
      ]
    }
  },
  "extras": [
    {
      "type": "STRING",
      "value": "\r\n"
    },
    {
      "type": "STRING",
      "value": "\n"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}